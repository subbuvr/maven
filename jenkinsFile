pipeline{
   agent any
       stages{
          stage("complie stage"){
          steps{
            withmaven(maven:'maven_3.5.4')
           {
               sh 'mvn clean complie'
             }
           }
        }
          stage("testing stage"){
          steps{
            withmaven(maven:'maven_3.5.4')
           {
               sh 'mvn test'
             }
           }
       }
     }
}
