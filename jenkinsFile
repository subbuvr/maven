pipeline{
   agent any
       stages("complie stage"){
          steps{
            withmaven(maven:'maven_3_5_0')
           {
               sh 'mvn clean complie'
             }
           }
        }
    stages("testing stage"){
          steps{
             withmaven(maven:'maven_3_5_0')
                {
                    sh 'mvn test'
              }
           }
       }
}
